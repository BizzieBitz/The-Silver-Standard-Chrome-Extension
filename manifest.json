{
  "manifest_version": 3,
  "name": "The Silver Standard",
  "description": "Converts prices in your browser from US dollars to ounces of silver",
  "version": "1.2",
  "permissions": [
      "storage"
      
  ],
  "icons": {
    "16": "SilverLogo.png",
    "48": "SilverLogo.png",
    "128": "SilverLogo.png"
},

  "content_scripts": [{
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_end"
  }]
}
